<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="458.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.seg12.model.VaultController">
   <children>
      <Label layoutX="81.0" layoutY="14.0" text="CONTENIDO">
         <font>
            <Font name="System Bold Italic" size="50.0" />
         </font>
      </Label>
      <Label layoutX="63.0" layoutY="89.0" text="Aqui se encuentra el contenido de su preciada caja fuerte:">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <TextArea fx:id="contentTA" layoutX="83.0" layoutY="122.0" prefHeight="200.0" prefWidth="293.0" promptText="Ingrese aquí su contenido...">
         <font>
            <Font size="14.0" />
         </font>
      </TextArea>
      <Button fx:id="closeBTN" layoutX="246.0" layoutY="334.0" mnemonicParsing="false" onAction="#cerrarCaja" text="Cerrar Caja">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Button>
      <Button fx:id="changeBTN" layoutX="83.0" layoutY="342.0" mnemonicParsing="false" onAction="#cambiarContraseña" prefHeight="31.0" prefWidth="153.0" text="Cambiar Contraseña">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
